{
  "name": "__MSG_extensionName__",
  "author": "Terry",
  "version": "0.1.4",
  "description": "__MSG_extensionDescription__",
  "manifest_version": 2,
  "default_locale": "en",
  "offline_enabled": true,

  "icons": {
    "19": "icon_19.png",
    "20": "icon_20.png",
    "24": "icon_24.png",
    "38": "icon_38.png",
    "48": "icon_48.png"
  },

  "permissions": [
    "<all_urls>", "bookmarks", "history", "webRequest", "webRequestBlocking"
  ],
  
  "background": {
    "scripts": ["background.js"]
  },

  "web_accessible_resources": [
    "redirect.htm"
  ]
}
